{
  "name": "hello-pepr-validate",
  "version": "0.0.1",
  "description": "Pepr feature: Validate",
  "keywords": [
    "pepr",
    "k8s",
    "policy-engine",
    "pepr-module",
    "security"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "pepr": {
