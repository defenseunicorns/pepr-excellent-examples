{
  "name": "hello-pepr-config-ignored-ns",
  "version": "0.0.1",
  "description": "Pepr Feature: Ignored NS Test",
  "keywords": [
    "pepr",
    "k8s",
    "policy-engine",
    "pepr-module",
    "security"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "pepr": {
    "uuid": "ignored-ns",
    "onError": "reject",
    "webhookTimeout": 10,
    "customLabels": {},
    "includedFiles": [],
    "env": {},
    "alwaysIgnore": {
      "namespaces": [
        "ignored"
      ]
    }
  },
  "dependencies": {
    "pepr": "0.51.6"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "vitest": "^3.2.4",
    "typescript": "5.8.3"
  },
  "scripts": {
    "pepr": "pepr",
    "cli": "npm run --workspace helpers cli -- --module=$(pwd)",
    "test": "npm run cli -- test --suite all",
    "test:e2e": "npm run cli -- test --suite e2e",
    "test:unit": "npm run cli -- test --suite unit"
  }
}
