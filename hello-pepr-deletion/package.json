{
  "name": "hello-pepr-deletion",
  "version": "0.0.1",
  "description": "Test the .WithDeletionTimestamp filter",
  "keywords": [
    "pepr",
    "k8s",
    "policy-engine",
    "pepr-module",
    "security"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "pepr": {
    "uuid": "0a646b44-c6af-5fea-b346-cd83b101889d",
    "onError": "reject",
    "webhookTimeout": 10,
    "customLabels": {
      "namespace": {
        "pepr.dev": ""
      }
    },
    "alwaysIgnore": {
      "namespaces": []
    },
    "includedFiles": [],
    "env": {}
  },
  "scripts": {
    "k3d-setup": "k3d cluster delete pepr-dev && k3d cluster create pepr-dev --k3s-arg '--debug@server:0' --wait && kubectl rollout status deployment -n kube-system"
  },
  "dependencies": {
    "pepr": "0.31.1"
  },
  "devDependencies": {
    "typescript": "5.3.3"
  }
}
