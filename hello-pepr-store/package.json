{
  "name": "hello-pepr-store",
  "version": "0.0.1",
  "description": "Pepr feature: Store",
  "keywords": [
    "pepr",
    "k8s",
    "policy-engine",
    "pepr-module",
    "security"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "pepr": {
    "uuid": "57067ccd-e94d-416f-a1c5-d57d983f3172",
    "onError": "reject",
    "webhookTimeout": 10,
    "alwaysIgnore": {
      "namespaces": [],
      "labels": []
    },
    "includedFiles": [],
    "env": {
      "LOG_LEVEL": "debug"
    }
  },
  "dependencies": {
    "pepr": "0.51.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^24.0.1",
    "jest": "^29.7.0",
    "typescript": "5.8.3"
  },
  "scripts": {
    "pepr": "pepr",
    "cli": "npm run --workspace helpers cli -- --module=$(pwd)",
    "test": "npm run cli -- test --suite all",
    "test:e2e": "npm run cli -- test --suite e2e",
    "test:unit": "npm run cli -- test --suite unit"
  }
}