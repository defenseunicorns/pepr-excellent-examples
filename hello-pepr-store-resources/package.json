{
  "name": "hello-pepr-store-resources",
  "version": "0.0.1",
  "description": "Pepr feature: Store Resources",
  "keywords": [
    "pepr",
    "k8s",
    "policy-engine",
    "pepr-module",
    "security"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "pepr": {
    "uuid": "store-resources",
    "onError": "reject",
    "webhookTimeout": 10,
    "alwaysIgnore": {
      "namespaces": [],
      "labels": []
    },
    "includedFiles": [],
    "env": {
      "LOG_LEVEL": "debug"
    }
  },
  "dependencies": {
    "pepr": "0.52.2"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "vitest": "^3.2.4",
    "typescript": "5.8.3"
  },
  "scripts": {
    "pepr": "pepr",
    "cli": "npm run --workspace helpers cli -- --module=$(pwd)",
    "test": "npm run cli -- test --suite all",
    "test:e2e": "npm run cli -- test --suite e2e",
    "test:unit": "npm run cli -- test --suite unit"
  }
}
